{
  "extends": "eslint:recommended",
  "env": {
    "es6": true,
    "browser": true,
    "jasmine": true
  },
  "globals": {
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "no-console": "off",
    "indent": [
      2,
      4,
      {
        "SwitchCase": 1
      }
    ],
    "no-unused-vars": [
      "error"
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "never"
    ]
  }
}
